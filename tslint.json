{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": false,
    "member-access": false,
    "interface-over-type-literal": false,
    "no-debugger": false,
    "max-classes-per-file": false,
    "no-var-requires": false,
    "jsx-no-lambda": false,
    "no-console": false
  }
}
